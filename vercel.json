{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.(css|js|html|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    },
    {
      "src": "/customer", 
      "dest": "/customer.html"
    },
    {
      "src": "/studio",
      "dest": "/studio.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
          

